ALTER FOREIGN TABLE testmulticorn alter test2 type bytea;
ALTER FOREIGN TABLE testmulticorn alter test1 type bytea;
select encode(test1, 'escape') from testmulticorn where test2 = 'test2 1 19'::bytea;
